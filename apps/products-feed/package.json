{
  "name": "saleor-app-products-feed",
  "version": "2.0.4",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check-types": "tsc",
    "deploy": "tsx --require dotenv/config ./scripts/deploy.ts",
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "fetch-schema": "curl https://raw.githubusercontent.com/saleor/saleor/${npm_package_saleor_schemaVersion}/saleor/graphql/schema.graphql > graphql/schema.graphql",
    "generate": "graphql-codegen",
    "init-db": "tsx --require dotenv/config ./scripts/init-db.ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "start": "next start",
    "test": "vitest",
    "test:ci": "vitest run --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.651.1",
    "@aws-sdk/client-s3": "^3.332.0",
    "@aws-sdk/lib-dynamodb": "3.651.1",
    "@aws-sdk/s3-request-presigner": "3.332.0",
    "@aws-sdk/util-dynamodb": "3.651.1",
    "@hookform/resolvers": "3.3.1",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/api-logs": "0.57.2",
    "@opentelemetry/instrumentation": "0.57.2",
    "@opentelemetry/resources": "1.30.1",
    "@opentelemetry/sdk-logs": "0.57.2",
    "@opentelemetry/sdk-trace-node": "1.30.1",
    "@opentelemetry/semantic-conventions": "1.30.0",
    "@saleor/app-sdk": "link:../../node_modules/@saleor/app-sdk",
    "@saleor/apps-logger": "workspace:*",
    "@saleor/apps-otel": "workspace:*",
    "@saleor/apps-shared": "workspace:*",
    "@saleor/apps-ui": "workspace:*",
    "@saleor/macaw-ui": "1.3.1",
    "@saleor/react-hook-form-macaw": "workspace:*",
    "@saleor/sentry-utils": "workspace:*",
    "@saleor/webhook-utils": "workspace:*",
    "@sentry/nextjs": "9.8.0",
    "@tanstack/react-query": "4.29.19",
    "@trpc/client": "10.43.1",
    "@trpc/next": "10.43.1",
    "@trpc/react-query": "10.43.1",
    "@trpc/server": "10.43.1",
    "@vercel/otel": "1.10.1",
    "@vitejs/plugin-react": "4.3.4",
    "dotenv": "16.3.1",
    "dynamodb-toolbox": "2.6.4",
    "fast-xml-parser": "^4.0.15",
    "graphql": "16.7.1",
    "graphql-tag": "2.12.6",
    "handlebars": "^4.7.7",
    "handlebars-helpers": "0.10.0",
    "jsdom": "20.0.3",
    "next": "15.2.4",
    "pg": "^8.16.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.43.9",
    "react-is": "18.2.0",
    "urql": "4.0.4",
    "usehooks-ts": "2.9.1",
    "vite": "6.2.4",
    "vitest": "3.1.1",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/introspection": "4.0.3",
    "@graphql-codegen/schema-ast": "4.0.2",
    "@graphql-codegen/typed-document-node": "5.0.5",
    "@graphql-codegen/typescript": "4.0.5",
    "@graphql-codegen/typescript-operations": "4.1.3",
    "@graphql-codegen/typescript-urql": "4.0.0",
    "@graphql-typed-document-node/core": "3.2.0",
    "@saleor/eslint-config-apps": "workspace:*",
    "@saleor/typescript-config-apps": "workspace:*",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.2.0",
    "@types/handlebars-helpers": "^0.5.6",
    "@types/pg": "^8.10.9",
    "@types/react": "18.2.5",
    "@types/react-dom": "18.2.5",
    "@vitest/coverage-v8": "3.1.1",
    "eslint": "9.23.0",
    "graphql-config": "5.0.3",
    "tsx": "4.19.3",
    "typescript": "5.8.2",
    "vite-tsconfig-paths": "5.1.4"
  },
  "saleor": {
    "schemaVersion": "3.10"
  }
}
